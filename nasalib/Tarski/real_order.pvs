real_order: THEORY

BEGIN

  x,y:VAR real
  epsil:VAR posreal

  real_ord_ep(epsil)(x,y): bool =
    floor(abs(x)/epsil) < floor(abs(y)/epsil)

  real_ord_ep_well_founded: JUDGEMENT
    real_ord_ep(epsil) has_type (well_founded?[real])

  real_ord_ep_decreases_halves: LEMMA
    abs(x)>=epsil IMPLIES
    real_ord_ep(epsil)(x/2,x)

END real_order
