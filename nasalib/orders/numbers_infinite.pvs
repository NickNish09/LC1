numbers_infinite: THEORY
 BEGIN

  nat_infinite: LEMMA NOT is_finite_type[nat]

  int_infinite: LEMMA NOT is_finite_type[int]

  rat_infinite: LEMMA NOT is_finite_type[rat]

  real_infinite: LEMMA NOT is_finite_type[real]

 END numbers_infinite
